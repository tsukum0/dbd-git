$c[Visual Studio Code

Name: ""
Command: ""
Description: ""

] 

$c[Variables to easy commands]
$var[in;$toLowercase[$message[1]]]
$var[2;$toLowercase[$message[2]]]

$nomention

$var[version;0]

$c[github client]
$c[Change the <link> <]$var[github;<link>]$c[>]
$if[$varExists[github]==true]
    $var[github;$getVar[github]]
    $endif

$c[github files]
$var[dbdgit;$var[github]/dbdgit]
$var[dbdgit_version;$var[github]/version]

$c[github update check]
$async[update_check]
    $httpGet[$var[dbdgit_version]]
    $endasync
$await[update_check]
$if[$httpResult>$var[version]]
    $endif

$c[client code]

$c[Load git request]
$httpGet[$var[github_code]]
$eval[$httpResult]